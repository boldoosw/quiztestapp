{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Charts/MatrixChart.jsx", "src/components/Charts/MChart.jsx", "src/app/dashboard_two/page.jsx", "src/app/dashboard_one/page.jsx", "src/app/dragdrop/page.jsx",  "src/components/TopicNavbar.jsx", "src/app/about8_9/page.jsx",  "src/components/UsersPage.jsx", "src/app/page.jsx", "src/app/api/auth/[...nextauth]/options.js", "src/app/matrix_test/page.jsx", "src/lib/session.js", "src/app/professions/static/page.jsx", "src/components/TopicsList.jsx"],
  "exclude": ["node_modules"]
}
